clear
draw()
{
	echo "********";
	echo "********";
	echo "********";
	echo "********";
	echo "********";
	echo ""
}

echo -n "How many square do you want? "
read num
for ((i=1;i<=num;++i))
do
	draw
	sleep 2
done

